msgid "  --disable-sound              Disable sound effects"
msgstr "  --disable-sound              効果音を無効にする"

#: src/supertux/command_line_arguments.cpp:111
msgid "  --disable-music              Disable music"
msgstr "  --disable-music              BGM を無効にする"

