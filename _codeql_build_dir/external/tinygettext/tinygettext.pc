prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/
includedir=${prefix}/include

Name: tinygettext
Description: tiny, minimal gettext replacement
Version: 
URL: https://github.com/tinygettext/tinygettext
Requires:
Libs: -L${libdir} -ltinygettext
Libs.private:
Cflags: -I${includedir}
